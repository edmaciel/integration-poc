<?xml version="1.0" encoding="UTF-8"?>

<beans:beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xmlns:int="http://www.springframework.org/schema/integration"
             xmlns:beans="http://www.springframework.org/schema/beans"
             xmlns="http://www.springframework.org/schema/beans"
             xsi:schemaLocation="http://www.springframework.org/schema/integration http://www.springframework.org/schema/integration/spring-integration.xsd
		            http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    
    <int:channel id="usage_input"/>
    <int:channel id="usage_reponse"/>

    <int:gateway id="userGateway"
             service-interface="com.integration.user.gateway.UserGateway"
             default-request-channel="usage_input"
             default-reply-channel="usage_reponse"/>
</beans:beans>